          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// ========================================",
                  "// STEP 1: Login - Extract and Save JWT Token",
                  "// ========================================",
                  "console.log('ðŸ” Processing login response...');",
                  "",
                  "if (pm.response.code === 200) {",
                  "    try {",
                  "        var jsonData = pm.response.json();",
                  "        console.log('ðŸ“¦ Response received:', JSON.stringify(jsonData, null, 2));",
                  "        ",
                  "        if (jsonData && jsonData.token) {",
                  "            // Save token to collection variable",
                  "            pm.collectionVariables.set('jwt_token', jsonData.token);",
                  "            ",
                  "            // Verify token was saved",
                  "            var savedToken = pm.collectionVariables.get('jwt_token');",
                  "            ",
                  "            console.log('âœ… STEP 1 COMPLETE: JWT Token saved successfully!');",
                  "            console.log('ðŸ“ Token (first 30 chars):', jsonData.token.substring(0, 30) + '...');",
                  "            console.log('ðŸ’¾ Token saved to variable: jwt_token');",
                  "            console.log('âœ… Token length:', jsonData.token.length, 'characters');",
                  "            ",
                  "            // Test assertion",
                  "            pm.test('âœ… Token saved to collection variable', function () {",
                  "                pm.expect(savedToken).to.not.be.empty;",
                  "                pm.expect(savedToken).to.equal(jsonData.token);",
                  "            });",
                  "            ",
                  "            pm.test('âœ… Token is valid JWT format', function () {",
                  "                pm.expect(jsonData.token.split('.')).to.have.lengthOf(3);",
                  "            });",
                  "        } else {",
                  "            console.error('âŒ ERROR: No token found in response!');",
                  "            console.error('Response structure:', JSON.stringify(jsonData, null, 2));",
                  "            pm.test('âŒ Response must contain token', function () {",
                  "                pm.expect(jsonData.token).to.exist;",
                  "            });",
                  "        }",
                  "    } catch (e) {",
                  "        console.error('âŒ ERROR: Failed to parse response:', e.message);",
                  "        console.error('Response text:', pm.response.text());",
                  "        pm.test('âŒ Response must be valid JSON', function () {",
                  "            pm.expect(e).to.be.null;",
                  "        });",
                  "    }",
                  "} else {",
                  "    console.error('âŒ ERROR: Login failed with status code:', pm.response.code);",
                  "    console.error('Response:', pm.response.text());",
                  "    console.log('ðŸ’¡ TIP: Check username and password are correct');",
                  "    console.log('ðŸ’¡ Default credentials: admin@academy.com / password123');",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
