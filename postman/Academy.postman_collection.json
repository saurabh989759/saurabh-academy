  "event": [
    {
      "listen": "prerequest",
      "script": {
        "exec": [
          "// ========================================",
          "// STEP 2: Pre-Request - Add JWT Token to Header",
          "// ========================================",
          "var requestUrl = pm.request.url.toString();",
          "var isAuthEndpoint = requestUrl.includes('/api/auth/');",
          "var isActuatorEndpoint = requestUrl.includes('/actuator/');",
          "var isPublicEndpoint = isAuthEndpoint || isActuatorEndpoint;",
          "",
          "console.log('ðŸŒ Request URL:', requestUrl);",
          "console.log('ðŸ”’ Is public endpoint:', isPublicEndpoint);",
          "",
          "if (!isPublicEndpoint) {",
          "    // Get token from collection variable",
          "    var token = pm.collectionVariables.get('jwt_token');",
          "    ",
          "    if (!token || token === '' || token === null || token === undefined) {",
          "        console.error('âŒ ERROR: No JWT token found in collection variable!');",
          "        console.error('ðŸ“‹ STEP 1 REQUIRED: Please run the \"Login\" request first!');",
          "        console.error('ðŸ“ Location: Authentication â†’ Login');",
          "        console.error('âš ï¸  This request will likely fail with 401/403 Unauthorized.');",
          "        ",
          "        // Try to get from environment variable as fallback",
          "        var envToken = pm.environment.get('jwt_token');",
          "        if (envToken) {",
          "            console.log('ðŸ’¡ Found token in environment variable, using it...');",
          "            token = envToken;",
          "            pm.collectionVariables.set('jwt_token', token);",
          "        }",
          "    }",
          "    ",
          "    if (token && token !== '' && token !== null && token !== undefined) {",
          "        // Remove any existing Authorization header",
          "        pm.request.headers.remove('Authorization');",
          "        ",
          "        // Add Authorization header with Bearer token",
          "        var authHeader = 'Bearer ' + token;",
          "        pm.request.headers.add({",
          "            key: 'Authorization',",
          "            value: authHeader",
          "        });",
          "        ",
          "        console.log('âœ… STEP 2 COMPLETE: JWT token added to Authorization header');",
          "        console.log('ðŸ“ Header: Authorization: Bearer ' + token.substring(0, 30) + '...');",
          "        console.log('ðŸ”‘ Token length:', token.length, 'characters');",
          "        ",
          "        // Verify header was added",
          "        var addedHeader = pm.request.headers.get('Authorization');",
          "        if (addedHeader) {",
          "            console.log('âœ… Verification: Authorization header is set');",
          "        } else {",
          "            console.error('âŒ Verification failed: Authorization header not found!');",
          "        }",
          "    } else {",
          "        console.error('âŒ Cannot proceed: No valid token available');",
          "    }",
          "} else {",
          "    console.log('â„¹ï¸  Public endpoint - No authentication required');",
          "}"
        ],
        "type": "text/javascript"
      }
    },
    {
      "listen": "test",
      "script": {
        "exec": [
          "// ========================================",
          "// STEP 3: Test - Verify Response",
          "// ========================================",
          "",
          "// Check for authentication errors",
          "if (pm.response.code === 401 || pm.response.code === 403) {",
          "    var errorType = pm.response.code === 401 ? 'Unauthorized' : 'Forbidden';",
          "    console.error('âŒ ERROR: ' + pm.response.code + ' ' + errorType);",
          "    console.error('ðŸ“‹ DIAGNOSIS:');",
          "    ",
          "    // Check if Authorization header was sent",
          "    var authHeader = pm.request.headers.get('Authorization');",
          "    if (!authHeader) {",
          "        console.error('   âŒ Authorization header is MISSING!');",
          "        console.error('   ðŸ“‹ SOLUTION: Run \"Login\" request first (Authentication â†’ Login)');",
          "    } else {",
          "        console.log('   âœ… Authorization header was sent:', authHeader.substring(0, 30) + '...');",
          "        ",
          "        // Check token format",
          "        if (!authHeader.startsWith('Bearer ')) {",
          "            console.error('   âŒ Authorization header format is wrong!');",
          "            console.error('   ðŸ“‹ Expected: Bearer {token}');",
          "            console.error('   ðŸ“‹ Found:', authHeader.substring(0, 20));",
          "        } else {",
          "            console.log('   âœ… Authorization header format is correct');",
          "            ",
          "            // Check if token might be expired",
          "            var token = pm.collectionVariables.get('jwt_token');",
          "            if (token) {",
          "                console.log('   ðŸ’¡ Token exists in collection variable');",
          "                console.log('   ðŸ’¡ TIP: Token might be expired. Run \"Login\" again to get fresh token');",
          "            }",
          "        }",
          "    }",
          "    ",
          "    console.error('ðŸ“‹ STEP-BY-STEP FIX:');",
          "    console.error('   1ï¸âƒ£ Go to Authentication folder');",
          "    console.error('   2ï¸âƒ£ Run \"Login\" request');",
          "    console.error('   3ï¸âƒ£ Check console for \"âœ… JWT Token saved successfully!\"');",
          "    console.error('   4ï¸âƒ£ Run this request again');",
          "    ",
          "    pm.test('âŒ Request should be authorized', function () {",
          "        pm.expect(pm.response.code).to.not.equal(401);",
          "        pm.expect(pm.response.code).to.not.equal(403);",
          "    });",
          "} else if (pm.response.code >= 200 && pm.response.code < 300) {",
          "    console.log('âœ… SUCCESS: Request completed with status', pm.response.code);",
          "    ",
          "    // Verify Authorization header was sent",
          "    var authHeader = pm.request.headers.get('Authorization');",
          "    if (authHeader) {",
          "        console.log('âœ… Authorization header was sent successfully');",
          "    }",
          "}",
          "",
          "// Log response time",
          "console.log('â±ï¸  Response time:', pm.response.responseTime, 'ms');"
        ],
        "type": "text/javascript"
      }
    }
